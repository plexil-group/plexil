AssignFailureWithConflict:
Concurrence
{
  Integer n = 0;
  Boolean spoil = false;
  Mutex n;
  PostCondition n == 2;

theFailure:
  {
    Priority 2;
    Using n;
    InvariantCondition !spoil;
    n = n + 1;
  }

theSpoiler:
  spoil = true;  

theConflict:
  {
    Priority 1;
    Using n;
    StartCondition spoil;
    n = 2;
  }

}
    