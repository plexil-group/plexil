// Compute Fibonacci(n)

Command pprint (...);

FibTest: {
  Integer n = 10;
  Integer result;

  if n < 2 result = n;
  else {
    Integer s1 = 0;
    Integer s2 = 1;
    for (Integer i = 2; i <= n; i + 1) {
      result = s1 + s2;
      s1 = s2;
      s2 = result;
    }
  }
  endif
  pprint (result);
}
