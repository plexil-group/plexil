Command front_engine_setTorque (Real);
Command rear_engine_setTorque (Real);
Real Lookup front_engine_Torque;
Real Lookup rear_engine_Torque;

Main:
{
  Real fTorque;
  Real bTorque;
  GetF: fTorque = Lookup (front_engine_Torque);
  GetB: bTorque = Lookup (rear_engine_Torque);
  SetF: front_engine_setTorque(bTorque);
  SetB: rear_engine_setTorque(fTorque);
}
