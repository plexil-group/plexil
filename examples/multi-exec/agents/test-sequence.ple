// -*- Mode: C++ -*-
// File: plexil/examples/multi-exec/agents/test-sequence.ple

TestSequence:
{
  Sequence:
  Recv1:
  {
    OnCommand recv_test (String arg1, Boolean arg2, Integer arg3, Real arg4)
    {
      Command print ("test-recv: arg1==", arg1, ", arg2==", arg2, ", arg3==", arg3, ", arg4==", arg4, "\n");
    }
  }
  Recv2:
  {
    StartCondition: Recv1.state == FINISHED;
    OnCommand recv_test (String arg1, Boolean arg2, Integer arg3, Real arg4)
    {
      Command print ("test-recv: arg1==", arg1, ", arg2==", arg2, ", arg3==", arg3, ", arg4==", arg4, "\n");
    }
  }
}

// EOF
