  (sequence "send-command"
    (interface (inout (real "realval" 0.0)))
    ;;(command "print" "\n[library] sent: lat\n")
    (action "get-value"
      (end-condition (not (= 0.0 (realvar "realval"))))
      (command-with-return (realvar "realval") "StateRequest" "lat"))
    ;;(command "print" "\n[library] recv: " (realvar "realval"))
    ))

