(plexil-plan
  (sequence "real-request"
    (interface (inout (string "name") (string "type") (real "realval")))
    (command "print" "[" (stringvar "name") ".real-request] sent: " (stringvar "type") "\n")
    (action "get-value"
      (end-condition (not (= 0.0 (realvar "realval"))))
      (command-with-return (realvar "realval") "StateRequest" (stringvar "type")))
    (command "print" "[" (stringvar "name") ".real-request] recv: " (realvar "realval") "\n")))

;; EOF
