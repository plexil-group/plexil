//The controlling plan
MasterPlan:
{
	Integer iter = 0;
	Integer ret[2];
	Sequence
		Action:
		{
			EndCondition: isKnown(ret[0]);
			Command: ret = GetNums(5);
		}
		Set:
		{
			Assignment: iter = ret[0] + ret[1];
		}
                NotivyViewer1: { Assignment: ret[0] = ret[0]; }
                NotivyViewer2: { Assignment: ret[1] = ret[1]; }
		Check:
		{
			InvariantCondition: isKnown(iter);
		}
}
