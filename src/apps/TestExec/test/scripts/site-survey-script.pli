(PlexilScript
  (Script
    (State "waypoint_x" "real" 1.0)
    (State "waypoint_y" "real" 1.0)
    (State "inspect_waypoint" "real" 2)
    (State "plan_paused" "bool" 0)
    (Command "get_next_inspection_point" "real" 0)
    (CommandAck "get_next_inspection_point" "string" "COMMAND_SUCCESS")
    (Command "move_to_waypoint" "real" 0
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string")
      (param 1.0 "real")
      (param 1.0 "real")
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string"))
    (CommandAck "move_to_waypoint" "string" "COMMAND_SUCCESS"
      (param "UNKNOWN" "string")
      (param "UNKNOWN" "string") (param 1.0 "real") (param 1.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (Command "acquire" "bool" 0)
    (CommandAck "acquire" "string" "COMMAND_SUCCESS")
    (UpdateAck "SendUpdate")
    (State "waypoint_x" "real" 2.0)
    (State "waypoint_y" "real" 2.0)
    (State "inspect_waypoint" "real" 1)
    (Command "get_next_inspection_point" "real" 0)
    (CommandAck "get_next_inspection_point" "string" "COMMAND_SUCCESS")
    (Command "move_to_waypoint" "real" 0 (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param 2.0 "real") (param 2.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (CommandAck "move_to_waypoint" "string" "COMMAND_SUCCESS" (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param 2.0 "real") (param 2.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (UpdateAck "SendUpdate")
    (State "waypoint_x" "real" 4.0)
    (State "waypoint_y" "real" 4.0)
    (State "inspect_waypoint" "real" 1)
    (Command "get_next_inspection_point" "real" 0)
    (CommandAck "get_next_inspection_point" "string" "COMMAND_SUCCESS")
    (State "plan_paused" "bool" 1)
    (command-abort "move_to_waypoint" "string" "COMMAND_SUCCESS" (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param 4.0 "real") (param 4.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (UpdateAck "SendUpdate")
    (State "plan_paused" "bool" 0)
    (State "waypoint_x" "real" 5.0)
    (State "waypoint_y" "real" 5.0)
    (State "inspect_waypoint" "real" 1)
    (Command "get_next_inspection_point" "real" 0)
    (CommandAck "get_next_inspection_point" "string" "COMMAND_SUCCESS")
    (CommandAck "move_to_waypoint" "string" "COMMAND_SUCCESS" (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param 5.0 "real") (param 5.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (Command "move_to_waypoint" "real" 0 (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param 5.0 "real") (param 5.0 "real") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string") (param "UNKNOWN" "string"))
    (UpdateAck "SendUpdate")
    ;;  (State "abort_plan" "bool" 1)
    ;;  (UpdateAck "MonitorAbortSignal")
    ;;  (UpdateAck "SignalEndOfPlan")
    (State "inspect_waypoint" "real" 0)
    (Command "get_next_inspection_point" "real" 0)
    (CommandAck "get_next_inspection_point" "string" "COMMAND_SUCCESS")
    (UpdateAck "SignalEndOfPlan")
    ))


