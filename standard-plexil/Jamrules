if ! $(PARSER_READY)
{
  local SUBDIR = $(parser) ;
  include [ FDirName $(SUBDIR) CommonRules ] ; 
  include [ FDirName $(SUBDIR) PlatformRules ] ;
  include [ FDirName $(SUBDIR) JavaRules ] ;
  include [ FDirName $(SUBDIR) AntlrRules ] ;
  include [ FDirName $(SUBDIR) Jamfile ] ;
  PARSER_READY = TRUE ;
}
