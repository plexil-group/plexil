        <Name>distance</Name>
        <Type>Integer</Type>
      </DeclareVariable>
      <DeclareVariable>
        <Name>direction</Name>
        <Type>Real</Type>
      </DeclareVariable>
    </VariableDeclarations>

    <UsingMutex>
      <Name>m</Name>
    </UsingMutex>

    <Name><StringValue>moveRover</StringValue></Name>
    <Node NodeType= "Empty">
      <NodeId>foo</NodeId>
    </Node>
  </OnCommand>
</PlexilPlan>
